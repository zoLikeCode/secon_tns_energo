{
  "expo": {
    "name": "seconhack",
    "slug": "seconhack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tns-czvet.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "platforms": ["ios", "android"],
    "android": {
      "networkSecurityConfig": "./network_security_config.xml",
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/unnamed.png",
        "backgroundColor": "#F0F0F0"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tns-czvet.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tns-czvet.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F0F0F0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
